<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/profileitem_detail_root"
    style="?android:attr/textAppearanceLarge"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:textIsSelectable="true"
    tools:context="com.opus.virna7.ProfileItemDetailFragment" >

    <TextView
        android:text="Comandar Instrumento"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/profiledetail_value_lb_header"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toTopOf="parent"
        android:textSize="36sp"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="16dp"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp" />


    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="463dp"
        android:id="@+id/profiledetail_value_guideline3"
        app:layout_constraintGuide_begin="53dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="53dp" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline4"
        app:layout_constraintGuide_begin="170dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="170dp" />

    <TextView
        android:text="Comando"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/profiledetail_value_textView3"
        android:textSize="18sp"
        android:layout_marginTop="96dp"
        app:layout_constraintLeft_toLeftOf="@+id/profiledetail_value_guideline3"
        app:layout_constraintTop_toTopOf="parent" />

    <Spinner
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:id="@+id/profiledetail_value_sp_valuetype"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="16dp"
        app:layout_constraintHorizontal_bias="0.0"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/guideline4"
        app:layout_constraintBottom_toBottomOf="@+id/profiledetail_value_textView3" />

    <TextView
        android:text="Valor 1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/profiledetail_value_lb_value1"
        android:textSize="18sp"
        android:layout_marginTop="56dp"
        app:layout_constraintTop_toBottomOf="@+id/profiledetail_value_textView3"
        app:layout_constraintLeft_toLeftOf="@+id/profiledetail_value_guideline3" />

    <EditText
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:text=""
        android:ems="10"
        android:id="@+id/profiledetail_value_et_value1"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="16dp"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="@+id/guideline4"
        app:layout_constraintBottom_toBottomOf="@+id/profiledetail_value_lb_value1" />

    <TextView
        android:text="Valor 2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/profiledetail_value_lb_value2"
        android:textSize="18sp"
        android:layout_marginTop="56dp"
        app:layout_constraintTop_toBottomOf="@+id/profiledetail_value_lb_value1"
        app:layout_constraintLeft_toLeftOf="@+id/profiledetail_value_guideline3" />

    <EditText
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:text=""
        android:ems="10"
        android:id="@+id/profiledetail_value_et_value2"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="16dp"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="@+id/guideline4"
        app:layout_constraintBottom_toBottomOf="@+id/profiledetail_value_lb_value2" />

    <TextView
        android:text="Valor 3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/profiledetail_value_lb_value3"
        android:textSize="18sp"
        android:layout_marginTop="56dp"
        app:layout_constraintTop_toBottomOf="@+id/profiledetail_value_lb_value2"
        app:layout_constraintLeft_toLeftOf="@+id/profiledetail_value_guideline3" />

    <EditText
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:text=""
        android:ems="10"
        android:id="@+id/profiledetail_value_et_value3"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="16dp"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="@+id/guideline4"
        app:layout_constraintBottom_toBottomOf="@+id/profiledetail_value_lb_value3" />





</android.support.constraint.ConstraintLayout>
